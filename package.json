{
  "name": "timesince",
  "version": "2.0.0",
  "description": "http://timesince.davidrutsch.com/",
  "main": "server.js",
  "scripts": {
    "test": "mocha -u tdd",
    "start": "node timesince.js"
  },
  "config": {
    "blanket": {
      "pattern": "///[\\w-]+\\.js$/",
      "data-cover-never": [
        "node_modules",
        "bower_components",
        "test"
      ],
      "data-cover-reporter-options": {
        "shortnames": true
      }
    }
  },
  "standard": {
    "ignore": []
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/theslip/timesince.git"
  },
  "keywords": [
    "moment",
    "timesince",
    "date",
    "timediff",
    "time",
    "diff",
    "duration",
    "node",
    "nodejs",
    "express",
    "react"
  ],
  "author": "David Rutsch",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/theslip/timesince/issues"
  },
  "homepage": "https://github.com/theslip/timesince",
  "devDependencies": {
    "grunt": "~0.4.5",
    "grunt-contrib-cssmin": "^0.13.0",
    "grunt-contrib-uglify": "^0.9.1",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-express-server": "^0.5.1",
    "grunt-newer": "^1.1.1",
    "grunt-jsbeautifier": "~0.2.10",
    "mocha": "~2.2.5",
    "chai": "~3.2.0",
    "sinon": "~1.16.0",
    "superagent": "~1.3.0",
    "grunt-simple-mocha": "~0.4.0",
    "blanket": "~1.1.7",
    "eslint": "~1.3.1",
    "react": "~0.13.3",
    "node-jsx": "~0.13.3",
    "eslint-plugin-react": "~3.3.0",
    "eslint-config-standard-react": "~1.0.4",
    "eslint-plugin-standard": "~1.3.0",
    "eslint-config-standard": "~4.1.0",
    "grunt-sass": "~1.0.0"
  },
  "dependencies": {
    "express": "^4.13.3",
    "body-parser": "~1.13.3",
    "moment": "~2.10.6",
    "lodash": "~3.10.1",
    "bunyan": "~1.4.0",
    "react": "~0.13.3",
    "node-jsx": "~0.13.3"
  }
}
