{
  "name": "timesince",
  "version": "2.0.0",
  "description": "A Date Calculator",
  "main": "server.js",
  "scripts": {
    "start": "node timesince.js",
    "browserify": "browserify ./app/config/browserify/scripts.js | uglifyjs -c > ./public/scripts/bundle.js",
    "sass": "node-sass ./app/styles/site.scss --output ./public/styles/ --output-style compressed --source-map true",
    "all": "npm run browserify && npm run sass"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/theslip/timesince.git"
  },
  "keywords": [
    "timesince",
    "date",
    "timediff",
    "time",
    "diff",
    "duration"
  ],
  "author": "David Rutsch",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/theslip/timesince/issues"
  },
  "homepage": "https://github.com/theslip/timesince",
  "dependencies": {
    "body-parser": "~1.13.3",
    "browserify": "^13.0.0",
    "express": "^4.13.3",
    "moment": "^2.11.2",
    "node-sass": "^3.4.2",
    "uglifyify": "^3.0.1"
  }
}
