{
  "name": "timesince",
  "version": "2.0.0",
  "description": "A Date Calculator",
  "main": "server.js",
  "scripts": {
    "start": "node timesince.js",
    "brow": "browserify -t babelify ./app/client.js | uglifyjs -c > ./public/scripts/bundle.js",
    "rf": "npm run brow && npm run start",
    "sass": "node-sass ./src/styles/site.scss --output ./src/.build/styles/ --output-style compressed --source-map true",
    "all": "npm run brow && npm run sass"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/theslip/timesince.git"
  },
  "keywords": [
    "timesince",
    "date",
    "timediff",
    "time",
    "diff",
    "duration"
  ],
  "author": "David Rutsch",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/theslip/timesince/issues"
  },
  "homepage": "https://github.com/theslip/timesince",
  "dependencies": {
    "babel-plugin-transform-strict-mode": "^6.6.5",
    "body-parser": "~1.13.3",
    "browserify": "^13.0.0",
    "core-js": "^2.2.0",
    "express": "^4.13.3",
    "moment": "^2.11.2",
    "node-jsx": "^0.13.3",
    "node-sass": "^3.4.2",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-helmet": "^2.3.1",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-thunk": "^1.0.3",
    "serialize-javascript": "^1.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.5.2",
    "babelify": "^7.2.0",
    "eslint": "^2.2.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-react": "^3.16.1",
    "eslint-plugin-standard": "^1.3.2",
    "uglifyify": "^3.0.1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  }
}
